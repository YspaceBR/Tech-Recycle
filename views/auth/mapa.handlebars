<html>
  <head>
    <title>Mapa com Google Maps API</title>
    <style>
      #map { height: 100vh; width: 100%; } { .tela-principal { padding: 0%;}
      .conteudo-auth { margin-bottom: 0%;} }
    </style>
  </head>
  <body>
    <div id="map"></div>

    <script>
      function initMap() { const localizacao = { lat: -15.7942, lng: -47.8822 };
      const mapa = new google.maps.Map(document.getElementById("map"), { zoom:
      12, center: localizacao, }); new google.maps.Marker({ position:
      localizacao, map: mapa, title: "Você está aqui!", }); }
    </script>

    <script
      src="https://maps.googleapis.com/maps/api/js?key=AIzaSyCJYf5k10Bh75U0nHK4riQgWax5KEW9PMc&callback=initMap"
      async
      defer
    >
    </script>
  </body>
</html>